{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "semicolon": [true, "never"],
    "quotemark": [true, "single"],
    "object-literal-sort-keys": [
      true,
      "ignore-case",
      "match-declaration-order"
    ],
    "prefer-const": [true, { "destructuring": "all" }]
  }
}
